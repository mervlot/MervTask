    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.mervlotx.myexpoapp"
    },
    "extra": {
      "eas": {
        "projectId": "6c28bd59-db0e-4447-bc0d-44c54bdaa247"
